{
  "name": "prisma-nexus-boilerplote",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "yarn run generate",
    "predev": "yarn run generate:prisma",
    "dev": "concurrently --kill-others \"yarn run queue\" \"yarn run server\"",
    "server": "cross-env NODE_ENV=development tsnd ./src",
    "queue": "tsnd src/queue.ts",
    "seed": "ts-node prisma/seed",
    "dev:typecheck": "tsc --watch --noEmit",
    "dev:migrate": "prisma migrate dev --preview-feature",
    "dev:resetdb": "prisma migrate reset --preview-feature",
    "dev:seed": "cross-env NODE_ENV=development ts-node prisma/seed",
    "prebuild": "yarn -s clean && yarn -s generate",
    "build": "tsc",
    "clean": "rimraf dist",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only ./src/schema",
    "start": "concurrently --kill-others \"cross-env NODE_ENV=production node dist/index.js\" \"node dist/queue.js\"",
    "studio": "dotenv -e .env.development.local -- prisma studio",
    "key:generate": "ts-node --transpile-only src/keys/generator.ts"
  },
